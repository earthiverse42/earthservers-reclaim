{
  "name": "@earthreclaim/api-client",
  "version": "0.1.0",
  "description": "API client for Earth Reclaim ratings server",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
